# NAV Scoring Environment Variables
# Copy to .env and fill in your values

# Zoho SMTP Configuration (for email notifications)
ZOHO_SMTP_USER=your-email@zoho.com
ZOHO_SMTP_PASSWORD=your-app-specific-password
ZOHO_SMTP_HOST=smtp.zoho.com
ZOHO_SMTP_PORT=587

# Database (auto-managed by SQLite, no changes needed)
DATABASE_URL=sqlite:///app/data/nav_scoring.db
